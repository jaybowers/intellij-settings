      <option name="COMMAND" value="$USER_HOME$/checkouts/hackday/go-startup/bin/am-auto-configure.sh" />
      <option name="PARAMETERS" />
      <option name="WORKING_DIRECTORY" value="$ProjectFileDir$" />
    </exec>
  </tool>
</toolSet>